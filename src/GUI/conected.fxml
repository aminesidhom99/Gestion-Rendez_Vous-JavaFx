<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ADD8E6;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ConectedController">

    <HBox prefHeight="40.0" prefWidth="600.0" spacing="10.0" style="-fx-background-color: #2a90FF;">
        <Region HBox.hgrow="ALWAYS" />
        <Button onAction="#showAll" prefHeight="53.0" prefWidth="207.0" style="-fx-background-color: transparent;" text="Profile" textFill="white">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
        <Button fx:id="logoutButton" style="-fx-background-color: transparent;" text="Log Out" textFill="white" mnemonicParsing="false" onAction="#handleLogOut">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetPasswordButton" style="-fx-background-color: transparent;" text="ResetPassword" textFill="white" mnemonicParsing="false" onAction="#handleResetPasswordButton">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
    </HBox>
                <Region prefHeight="600.0" prefWidth="200.0" style="-fx-background-color: #2a90FF;" />

   <Region prefHeight="400.0" prefWidth="200.0" rotate="180.0" />
   

</AnchorPane>
